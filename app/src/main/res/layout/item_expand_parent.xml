<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="10dp"
    android:id="@+id/item_test_list_expand_parent_root"
    android:background="@color/mtf_gray_200"
    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <View
            android:id="@+id/item_test_list_expand_parent_view"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@mipmap/icon_checkbox_nor"
            />
        <TextView
            android:id="@+id/item_test_list_expand_name_parent_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            style="@style/TextHeader"
            android:text="机油"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10dp"
            />
    </LinearLayout>
</LinearLayout>