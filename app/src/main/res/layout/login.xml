<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        tools:context="com.proton.bystone.ui.main.tab.MeFragment">

        <!-- TODO: Update blank fragment layout -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            >

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal"
                >
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/icon_return"
                    android:paddingLeft="10dp"
                    android:layout_centerVertical="true"

                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="账号密码登陆"
                    android:paddingLeft="130dp"
                    android:textSize="18sp"
                    android:textColor="#878787"
                    android:layout_centerVertical="true"
                    />

            </RelativeLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@mipmap/icon_mai_line"
                />
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="59dp"
                android:orientation="horizontal"
                android:padding="5dp"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/icon_user"
                    android:layout_marginLeft="10dp"
                    android:layout_centerVertical="true"

                    />
                <EditText
                    android:id="@+id/editText"
                    android:layout_width="240dp"
                    android:layout_height="wrap_content"
                    android:hint="18225026697"
                    android:textSize="15sp"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="35dp"
                    android:text="18225026697"

                    android:background="@null" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="2dp"
                    android:background="#ebebeb"
                    android:layout_marginTop="60dp"
                    android:layout_below="@id/bt"/>
            </RelativeLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#ebebeb"
                />



            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="59dp"
                android:orientation="horizontal"
                android:padding="5dp"
                >

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/icon_pwd"
                    android:layout_marginLeft="10dp"
                    android:layout_centerVertical="true"

                    />
                <EditText
                    android:layout_width="240dp"
                    android:layout_height="wrap_content"
                    android:text="666888"
                    android:textSize="15sp"
                    android:id="@+id/et2"

                    android:background="@null"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="35dp"/>

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/icon_uneyes"
                    android:paddingTop="25dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"/>


            </RelativeLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="#ebebeb"

                />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                >


            </RelativeLayout>
            <Button
                android:layout_width="337dp"
                android:layout_height="wrap_content"
                android:text="登陆"
                android:id="@+id/btdenglu"
                android:layout_gravity="center_horizontal"
                android:textSize="15sp"
                android:background="@drawable/yuanjiao2"
                android:layout_marginTop="35dp"
                android:textColor="#ffff"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="70sp"
                android:orientation="horizontal"

                >
                <Button
                    android:id="@+id/wj"
                    android:layout_width="wrap_content"
                    android:layout_height="60sp"
                    android:text="忘记密码"
                    android:layout_marginTop="32dp"
                    android:textSize="15sp"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    android:textColor="#878787"
                    />

                <Button
                    android:id="@+id/gg"
                    android:layout_width="wrap_content"
                    android:layout_height="60sp"
                    android:text="更改密码"
                    android:layout_marginTop="32dp"
                    android:textSize="15sp"
                    android:layout_centerVertical="true"
                    android:background="@null"
                    android:layout_marginLeft="240dp"
                    android:textColor="#878787"
                    />


            </RelativeLayout>



        </LinearLayout>

    </FrameLayout>


